{
  "name": "jwohlfert23/laravel-api-query",
  "description": "Provides laravel scope to build query from request query params",
  "keywords": [
    "laravel",
    "api",
    "query"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jack Wohlfert",
      "email": "jack.wohlfert@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/jwohlfert23/laravel-api-query.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jwohlfert23\\LaravelApiQuery\\": "src/"
    }
  },
  "require": {
    "php": "^8.2",
    "nesbot/carbon": "~3",
    "illuminate/database": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0",
    "symfony/http-foundation": "^7.0",
    "kirschbaum-development/eloquent-power-joins": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^10.0"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
